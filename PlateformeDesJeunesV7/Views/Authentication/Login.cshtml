<body>
    <!-- Page content -->
    <div class="container d-flex flex-column">
        <div class="row align-items-center justify-content-center no-gutters min-vh-100">
            <div class="col-lg-5 col-md-8 py-8 py-xl-0">
                <!-- Card -->
                <div class="card shadow ">
                    <!-- Card body -->
                    <div class="card-body p-6">
                        <div class="mb-4">
                            <div style="display:flex;justify-content:center;">
                                <div><a asp-area="Identity" asp-page="/Account/Login"><img src="~/assets/images/logo143.jpeg" class="mb-1" alt="Plateforme des jeunes Ain Sebaa - Amélioration du Revenu et Inclusion Économique des Jeunes" style="width:150px;height:150px;"></a></div>
                            </div>
                            
                            <h1 class="mb-1 font-weight-bold">Plateforme des jeunes</h1>
                            <h3 class="mb-1 font-weight-bold">Authentification</h3>
                            <span>Vous n'avez pas de compte? <a asp-action="Register" asp-controller="Authentication" class="ml-1" style="color:#00AFF5!important;">Inscription</a></span>
                        </div>
                        <!-- Form -->
                        <form method="post">
                            <!-- Username -->
                            <div class="form-group">
                                <label for="email" class="form-label">Email</label>
                                <input asp-for="UserName" type="text" id="UserName" class="form-control" placeholder="Votre nom d'utilisateur ici" required>
                                <div class="invalid-feedback">
                                    Veuillez entrer votre nom d'utilisateur
                                </div>
                                <span asp-validation-for="UserName" class="text-danger"></span>
                            </div>
                            <!-- Password -->
                            <div class="form-group">
                                <label for="password" class="form-label">Mot de passe</label>
                                <input asp-for="Password" type="password" id="password" class="form-control" placeholder="**************"
                                       required>
                                <div class="invalid-feedback">
                                    Veuillez entrer le mot de passe
                                </div>
                                <span asp-validation-for="Password" class="text-danger"></span>
                            </div>
                            <!-- Checkbox -->
                           @* <div class="d-lg-flex justify-content-between align-items-center mb-4">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" id="rememberme">
                                    <label class="custom-control-label " for="rememberme">Se souvenir de moi</label>
                                </div>
                                <div>
                                    <a href="forget-password.html">Forgot your password?</a>
                                </div>
                            </div>*@
                            <div id="recaptcha_div"></div>

                            <br />
                            <div id="mesBoutons">
                                @*<div class="g-recaptcha" data-sitekey="6LeYP9ciAAAAALwP8XXPMdJWwCoKOPWd8OeC1dx5"></div>*@

                                <h5 style="color:darkgreen;text-decoration: underline;">@ViewData["ClientPublicSucess"]</h5>
                                <h5 style="color:#ff0000b3;text-decoration: underline;">@TempData["LoginError"]</h5>
                                <!-- Button -->
                                <button type="submit" class="btn btn-block" style="background-color:#00AFF5;color:white;">S'authentifier</button>
                                <a class="btn btn-block attention" style="background-color:#0074A3;color:white;" asp-controller="DemandCandidate" asp-action="Add">Poster ma candidature</a>

                            </div>
                            <hr class="my-4">
                            <div class="font-size-md mt-4 text-center">
                                <a href="https://www.facebook.com/منصة-الشباب-عين-السبع-107323618188959/" class="fab fa-facebook text-muted mr-3"></a>
                                <a href="https://instagram.com/plateformedesjeunes.as?igshid=YmMyMTA2M2Y=" class="fab fa-instagram text-muted mr-3"></a>
                                <a href="https://www.linkedin.com/company/plateforme-des-jeunes-ain-sbaa/" class="fab fa-linkedin text-muted "></a>
                            </div>
      
                        </form>
                    </div>
                </div>           
            </div>
        </div>
